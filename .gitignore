cat > .gitignore <<'EOF'
# Node dependencies
node_modules/

# Environment / secrets
.env
.env.local
.env.*.local

# Frontend build outputs
frontend/build/
build/
dist/

# Logs
npm-debug.log*
yarn-debug.log*
pnpm-debug.log*
*.log

# OS files
.DS_Store
Thumbs.db

# Editors / IDEs
.vscode/
.idea/

# Test / coverage
coverage/

# Misc
.cache/
*.tgz
EOF
